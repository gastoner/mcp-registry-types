{
  "name": "@kortex-hub/mcp-registry-types",
  "version": "1.4.0-next",
  "description": "TypeScript types for the Model Context Protocol (MCP) Registry API",
  "repository": "https://github.com/kortex-hub/mcp-registry-types",
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "vite build",
    "fetch": "ts-node src/fetch-schema.ts",
    "generate:schema": "npx openapi-typescript openapi.yaml -o src/schema.d.ts --export-type",
    "generate:json": "ts-node src/convert-yaml-to-json.ts",
    "generate": "pnpm run fetch && pnpm run generate:schema && pnpm run generate:json"
  },
  "dependencies": {
    "ajv": "^8.17.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24",
    "yaml": "^2.8.1",
    "openapi-typescript": "^7.8.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.3.0"
  },
  "packageManager": "pnpm@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a"
}
